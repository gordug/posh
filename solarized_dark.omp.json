{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "final_space": false,
    "console_title": true,
    "console_title_style": "template",
    "console_title_template": "{{ .SessionState.Path }}",
    "blocks": [
        {
            "type": "prompt",
            "alignment": "left",
            "segments": [
                {
                    "type": "root",
                    "style": "powerline",
                    "powerline_symbol": "\uE0B0",
                    "foreground": "#073642",
                    "background": "#eee8d5",
                    "properties": {
                        "root_icon": "üëë"
                    }
                },
                {
                    "type": "path",
                    "style": "powerline",
                    "powerline_symbol": "\uE0B0",
                    "foreground": "#073642",
                    "background": "#eee8d5",
                    "properties": {
                        "folder_icon": "üìÅ",
                        "style": "mixed",
                        "home_icon": "üè†",
                        "home_style": "folder"
                    }
                }
            ]
        },
        {
            "type": "prompt",
            "alignment": "right",
            "segments": [
                {
                    "type": "time",
                    "style": "plain",
                    "foreground": "#268bd2",
                    "background": "#002b36",
                    "properties": {
                        "time_format": "15:04:05",
                        "prefix": "<#93a1a1>\uE868</>"
                    }
                }
            ]
        },
        {
            "newline": true,
            "type": "prompt",
            "alignment": "left",
            "segments": [
                {
                    "type": "os",
                    "style": "powerline",
                    "powerline_symbol": "\uE0B0",
                    "foreground": "#268bd2",
                    "background": "#eee8d5"
                },
                {
                    "type": "git",
                    "style": "powerline",
                    "powerline_symbol": "\uE0B0",
                    "foreground": "#6c71c4",
                    "background": "#eee8d5",
                    "properties": {
                        "symbol": "ÓÇ†",
                        "display_status": true,
                        "display_stash_count": true,
                        "display_upstream_icon": true
                    }
                },
                {
                    "type": "project",
                    "style": "powerline",
                    "powerline_symbol": "\uE0B0",
                    "foreground": "#2aa198",
                    "background": "#eee8d5",
                    "properties": {
                        "symbol": "üì¶",
                        "style": "mixed"
                    }
                },
                {
                    "type": "python",
                    "style": "powerline",
                    "powerline_symbol": "\uE0B0",
                    "foreground": "#dc322f",
                    "background": "#eee8d5",
                    "properties": {
                        "symbol": "üêç",
                        "style": "mixed",
                        "virtualenv_icon": "üêç",
                        "virtualenv_prefix": "(",
                        "virtualenv_suffix": ")",
                        "conda_icon": "üêç",
                        "conda_prefix": "(",
                        "conda_suffix": ")"
                    }
                },
                {
                    "type": "dotnet",
                    "style": "powerline",
                    "powerline_symbol": "\uE0B0",
                    "foreground": "#b58900",
                    "background": "#eee8d5",
                    "properties": {
                        "symbol": ".NET",
                        "style": "mixed"
                    }
                },
                {
                    "type": "node",
                    "style": "powerline",
                    "powerline_symbol": "\uE0B0",
                    "foreground": "#268bd2",
                    "background": "#eee8d5",
                    "properties": {
                        "symbol": "‚¨¢",
                        "style": "mixed"
                    }
                },
                {
                    "type": "java",
                    "style": "powerline",
                    "powerline_symbol": "\uE0B0",
                    "foreground": "#cb4b16",
                    "background": "#eee8d5",
                    "properties": {
                        "symbol": "‚òï",
                        "style": "mixed"
                    }
                },
                {
                    "type": "rust",
                    "style": "powerline",
                    "powerline_symbol": "\uE0B0",
                    "foreground": "#b58900",
                    "background": "#eee8d5",
                    "properties": {
                        "symbol": "ü¶Ä",
                        "style": "mixed"
                    }
                },
                {
                    "type": "docker",
                    "style": "powerline",
                    "powerline_symbol": "\uE0B0",
                    "foreground": "#268bd2",
                    "background": "#eee8d5",
                    "properties": {
                        "symbol": "üê≥",
                        "style": "mixed"
                    }
                }
            ]
        }
    ]
}
